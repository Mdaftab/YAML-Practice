# Complex Application Environment Variables

# Application Configuration
NODE_ENV=production
LOG_LEVEL=info
API_TIMEOUT=30
MAX_CONNECTIONS=1000
CACHE_TTL=3600

# Feature Flags
ENABLE_PAYMENTS=true
ENABLE_ANALYTICS=true
ENABLE_BETA_FEATURES=false
MAINTENANCE_MODE=false

# Database Configuration
DATABASE_URL=postgresql://db_user:secure_password@db.example.com:5432/myapp
DB_HOST=db.example.com
DB_USER=db_user
DB_PASSWORD=secure_password
DB_POOL_SIZE=20

# API Secrets
STRIPE_API_KEY=sk_live_abc123xyz789
STRIPE_WEBHOOK_SECRET=whsec_webhook_secret_123
JWT_SECRET=very_secure_jwt_secret_key_min_32_chars
API_KEY=api_key_secure_12345

# Logging Configuration
LOG_ENDPOINT=https://logs.example.com/api
LOGGING_API_KEY=logging_api_key_123
