# Staging Environment Variables
APP_ENV=staging
SERVER_HOST=0.0.0.0
SERVER_PORT=8080
SSL_ENABLED=true

# Staging Database
DB_HOST=staging-db.example.com
DB_PORT=5432
DB_NAME=myapp_staging
DB_USER=staging_user
DB_PASSWORD=staging_secure_password
DB_POOL_MIN=5
DB_POOL_MAX=20

# Staging API
API_BASE_URL=https://staging-api.example.com
API_TIMEOUT=30
API_KEY=staging_api_key_xyz789

# Staging Cache
CACHE_PROVIDER=redis
CACHE_HOST=staging-redis.example.com
CACHE_PORT=6379
CACHE_TTL=3600

# Staging Logging
LOG_LEVEL=info
LOG_FORMAT=json
