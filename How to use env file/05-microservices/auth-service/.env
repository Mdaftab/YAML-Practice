# Auth Service Environment Variables
AUTH_SERVICE_PORT=3001
AUTH_DB_HOST=auth-db.example.com
AUTH_DB_PORT=5432
AUTH_DB_NAME=auth_db
AUTH_DB_USER=auth_user
AUTH_DB_PASSWORD=auth_secure_password
AUTH_DB_URL=postgresql://auth_user:auth_secure_password@auth-db:5432/auth_db
JWT_SECRET=very_secure_jwt_secret_for_auth
JWT_EXPIRATION=3600
JWT_ALGORITHM=HS256
REDIS_HOST=redis.example.com
REDIS_PORT=6379
REDIS_URL=redis://redis.example.com:6379
LOG_LEVEL=info
